    "def cel2fah(celsius):\n",
    "    fahrenheit = (9/5) * celsius + 32\n",
    "    return fahrenheit\n",
    "\n",
    "for celsius in range(0, 61, 10):\n",
    "    fahrenheit = cel2fah(celsius)\n",
    "    print(f\"섭씨온도는 {celsius}도 = 화씨온도는 {fahrenheit}도\")\n",
    "    print(\"\")"
   ]
